{
  "name": "@acphast/config",
  "version": "0.1.0",
  "description": "Configuration loading and validation for Acphast",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist *.tsbuildinfo"
  },
  "dependencies": {
    "@acphast/core": "workspace:*",
    "zod": "^3.22.4",
    "smol-toml": "^1.1.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
