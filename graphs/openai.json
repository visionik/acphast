{
  "version": "1.0.0",
  "metadata": {
    "name": "OpenAI Pipeline",
    "description": "Disaggregated OpenAI pipeline",
    "author": "Acphast",
    "created": "2026-02-04"
  },
  "nodes": [
    {
      "id": "input",
      "type": "ACP Input",
      "position": { "x": -150, "y": 200 },
      "config": {}
    },
    {
      "id": "translator",
      "type": "OpenAI Translator",
      "position": { "x": 100, "y": 200 },
      "config": {
        "defaultModel": "gpt-4-turbo",
        "defaultMaxTokens": 4096
      }
    },
    {
      "id": "client",
      "type": "OpenAI Client",
      "position": { "x": 400, "y": 200 },
      "config": {}
    },
    {
      "id": "normalizer",
      "type": "OpenAI Normalizer",
      "position": { "x": 700, "y": 200 },
      "config": {
        "includeModel": true,
        "includeId": true
      }
    },
    {
      "id": "output",
      "type": "ACP Output",
      "position": { "x": 950, "y": 200 },
      "config": {}
    }
  ],
  "connections": [
    {
      "source": "input",
      "sourceOutput": "out",
      "target": "translator",
      "targetInput": "in"
    },
    {
      "source": "translator",
      "sourceOutput": "out",
      "target": "client",
      "targetInput": "in"
    },
    {
      "source": "client",
      "sourceOutput": "out",
      "target": "normalizer",
      "targetInput": "in"
    },
    {
      "source": "normalizer",
      "sourceOutput": "out",
      "target": "output",
      "targetInput": "in"
    }
  ]
}
